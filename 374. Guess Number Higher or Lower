典型二分法的题
public class Solution extends GuessGame {
    public int guessNumber(int n) {
    int high=n;
    int low=1;
    
    while(low<=high){
        int middle=low+(high-low)/2;
    if(guess(middle)==1)
        low=middle+1;
    else if(guess(middle)==-1)
        high=middle-1;
    else     
    return middle; 
    }
        return -1;
}
    }
