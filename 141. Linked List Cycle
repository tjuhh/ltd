/*基本思想用一个hashset存起来
*/

public class Solution {
    public boolean hasCycle(ListNode head) {
        Set<ListNode> visited=new HashSet<ListNode>();
  
        while(head!=null)
        {if(!visited.add(head))
         return true;
         else
         head=head.next;
            
        }
        return false;
    }
}
